{
  "name": "@hls-app/web",
  "root": "apps/web",
  "sourceRoot": "apps/web/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run dev",
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run build",
        "cwd": "apps/web",
        "outputs": [
          "apps/web/dist"
        ]
      }
    }
  }
}